/* components/line-time/line-time.wxss */
page {
  background-color: #f4f4f4;
  line-height: 100rpx;
}
.main {
  border-bottom: 1px #f4f4f4 solid;
  background-color: #ffffff;
  width: 100%;
  height: 100rpx;
}
.title {
  padding-left: 40rpx;
  width: 25%;
  display: inline-block;
  border-right: 1px #f4f4f4 solid;
}
.left {
  border-right: 1px #f4f4f4 solid;
}
picker {
  font-size: 28rpx;
  color: #888888;
  margin-left: 20rpx;
  width: 23%;
  height: 100rpx;
  display: inline-block;
}